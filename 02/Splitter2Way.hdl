/**
 * 2-way splitter
 * out = {in, in, in}
 */

CHIP Splitter2Way {
    IN in;
    OUT out[2];

    PARTS:
    And(a=in, b=true, out=out[0]);
    And(a=in, b=true, out=out[1]);
}
