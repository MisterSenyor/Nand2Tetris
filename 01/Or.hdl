 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=n_a);
    Not(in=b, out=n_b);
    And(a=n_a, b=n_b, out=n_a_and_n_b);
    Not(in=n_a_and_n_b, out=out);
}
