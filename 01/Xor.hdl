/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Nand(a=a, b=b, out=a_nand_b);
    Or(a=a, b=b, out=a_or_b);
    And(a=a_nand_b, b=a_or_b, out=out);
}